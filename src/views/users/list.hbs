<link rel="stylesheet" href="">
{{#each lista}}
<form action="/user/list/{{idPage}}" method="get">
    {{/each}}
    <div id="contenedor">
        <div id="titulo">
            <p>Lista de Usuarios</p>
        </div>
        {{#each listaPagina}}
         <div id="mas">
            <a href="/user/add/{{idPage}}">+</a>
        </div>
        {{/each}}
        <div id="Datos">
            <div class="row">
                {{#each lista}}
                <div id="tarjeta">
                    <div id="foto">
                        <img src="/img/usuario/{{photoUser}}">
                    </div>
                    <div id="nombre">
                        <p>{{completeNameUser}}</p>
                    </div>
                    <div id="descripcion">
                        <p>{{identificationCardUser}}</p>
                    </div>
                    <div id="correo">
                        <p>{{emailUser}}</p>
                    </div>
                    <div id="celular">
                        <p>{{cellPhoneUser}}</p>
                    </div>
                    <div id="usuario">
                        <p>{{usernameUser}}</p>
                    </div>
                    <div id="contraseÃ±a">
                        <p>{{passwordUser}}</p>
                    </div>
                    <div id="estado">
                        <p>{{stateUser}}</p>
                    </div>
                    <div id="rol">
                        <p>{{nameRol}}</p>
                    </div>
                    <div id="permisos">
                        <p>{{namePermission}}</p>
                    </div>
                    <div id="actualizar">
                        <a href="/user/update/{{pageIdPage}}"><button type="button">Actualizar</button></a>
                    </div>
                    <div id="desavilitar">
                        <a href="/user/desability/{{pageIdPage}}"><button type="button">Desabilitar</button></a>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</form>