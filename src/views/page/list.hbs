<link rel="stylesheet" href=""> 
{{#each listaPagina}}
<form action="/page/list/{{idPage}}" method="get">
    <div id="contenedor">
        <div id="titulo">
            <p>Lista Pagina N째 {{idPage}}</p>
        </div>
        <div id="logo">
            <div id="imagen">
                <div id="cambioImagen">
                    <img src="/img/page/{{photoPage}}" id="imagenPrevisualizacion" />
                </div>
            </div>
            <div id="Nombre">
                <p>Nombre de la Pagina:</p>
                <input type="text" name="namePage" id="namePage" required value="{{namePage}}">
            </div>
            <div id="descripcion">
                <p>Descripci처n:
                <textarea name="descriptionPage" id="descriptionPage">{{descriptionPage}}</textarea>
                </p>
            </div>
            <div id="mision">
                <p>Misi처n:</p>
                <textarea name="misonPage" id="misonPage">{{misonPage}}</textarea>
            </div>
            <div id="vision">
                <p>Visi처n:</p>
                <textarea name="visonPage" id="visonPage">{{visonPage}}</textarea>
            </div>
            <div id="celular">
                <p>Celular:</p>
                <input type="text" name="cellPhonePage" id="cellPhonePage" value="{{cellPhonePage}}">
            </div>
            <div id="correo">
                <p>Correo Electronico:</p>
                <input type="email" name="emailPage" id="emailPage" value="{{emailPage}}">
            </div>
            <div id="boton">
                <a href="/page/Update/{{idPage}}"><button type="button">Actualizar</button></a>
            </div>
        </div>
    </div>
    
</form>
{{/each}}